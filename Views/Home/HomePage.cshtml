                        </div>
                    <p style="color:red; font-size:20px" id="ErrorMessage"></p>
                        <button onclick="loginAuthentication();" id="loginBtn" type="submit" class="btn btn-success"><span class="fa fa-lock"></span> Login</button>
                    @*</form>*@
                    <script>
                        function loginAuthentication() {
                            var userName = $('#chuss').val();
                            var passwrod = $('#password').val();
                            
                            var data = {};
                            data.Email = userName;
                            data.Password = passwrod;
                            $.ajax({
                                type: "POST",
                                url: "/Home/UserLogin",
                                async: true,
                                data: data,
                                success: function (data) {
                                    if (data) {
                                        //alert("Success" + data);
                                        window.location.replace("/Home/Index");
                                    } else {
                                        $("#ErrorMessage").html("<b>Invalid UserName or Password</b>");
                                    }
                                },
                                error: function () { }
                            });
                        }
                    </script>
