    <!-- Full Page Image Background Carousel Header -->
    <header id="myCarousel" class="carousel slide">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for Slides -->
        <div class="carousel-inner">
            <div class="item">
                <!-- Set the first background image using inline CSS below. -->
                <div class="fill" style="background-image: url(/Images/Globe.jpg);"></div>

                <div class="carousel-caption">
                    <div style="border:groove; padding:35px">
                        <h3>Data Visualization is a powerful way to simplify the complexity in our data and present it in a form which is comprehensible, insightful and actionable</h3>
                    </div>
                    <br /><br /><br /><br /><br /><br /><br /><br />
                </div>
            </div>
            <div class="item">
                <!-- Set the second background image using inline CSS below. -->
                <div class="fill" style="background-image: url(/Images/bbkk2.jpg);"></div>
                <div class="carousel-caption">
                    <div style="border:groove; padding:35px">
                        <h1 class="intro-heading">Numbers have an important story to tell. </h1>
                        <h1>They rely on you to give them a clear and convincing voice.</h1>
                    </div>
                    <br /><br /><br /><br /><br /><br /><br /><br />
                </div>
            </div>
            <div class="item active">
                <!-- Set the third background image using inline CSS below. -->
                <div class="fill" style="background-image: url(/Images/bkbk.jpg);"></div>
                <div class="carousel-caption">
                    <div style="border:groove">
                        <br />
                        <h1>The purpose of visualization is</h1>
                        <h1>insight, not pictures.</h1>
                        <br />
                    </div>
                    <br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
                </div>
            </div>
        </div>

        <!-- Controls -->
