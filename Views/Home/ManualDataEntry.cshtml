            function addRowFn() {
                var table = document.getElementById("NewAttrTable");
                $('#NewAttrTable tbody').append($("#NewAttrTable tbody tr:last").clone());
                $('#NewAttrTable tbody tr:last :text');
                $('#NewAttrTable tbody tr:last td:first').html(--table.rows.length);
            }

            function addColumnFn() {
                $('#NewAttrTable tr').append($("<td>"));
                $('#NewAttrTable thead tr td:last').html('<input class="form-control" type="text" placeholder="Column Name" />');
                $('#NewAttrTable tbody tr').each(function () { $(this).children('td:last').append($('<input class="number-only form-control" type="text" />')) });
            }

            function submitData(colName, list) {
                var AttributeName = $("#Title").val();
                var LocationName = $("#Location").val();
                // colName = column names, list == data
                var data = [];

                if (AttributeName == "")
                {
                    swal("'Attribute' Is Empty");
                }
                else if (LocationName == "") {
                    swal("'Location' Is Empty");
                }
                else {
                    var zzz = list;
                    alert("No of Rows = " + zzz.length);

                    var send = new Array;
                    for(var a = 0; a < zzz.length; a++)
                    {
                        for (var b = 0; b < zzz[a].length; b++)
                        {
                            send.push(zzz[a][b]);
                        }
                    }
                    alert("Send Contains : " + send);
                    $.ajax({
                        url: "/Home/PlaceData/?y=" + colName + "&z=" + send + "&name=" + AttributeName + "&loc=" + LocationName,
                        type: "POST",
                        data: JSON.stringify(data),
                        dataType: "JSON",
                        contentType: "Application/JSON",
                        async: true,
                        success: function (d) {
                            alert("Column Names Return = " + d);
                        }
                        , error: function () {
                            alert('Sorry... Data Entry Fails..');
                        }
                    });

                    $("#buttonz").html("");
                    $("#buttonz").append("<br/>Your Provided Data Has Been Sent To NDV. Thank You.<br/>");
                    $("#buttonz").append("<br/><input class='btn-success' type='button' onclick='moreRecords()' value='Enter More Records' />");
                }
            }

            function collectData() {
                var table = document.getElementById("NewAttrTable");
                var count = 0;
                var colName = new Array;
                var list = new Array();
                //var list = [];

                for (var x = 1; ; x++)
                {
                    try
                    {
                        if (table.rows[0].cells[x].firstChild.value) {
                            colName.push(table.rows[0].cells[x].firstChild.value);
                        }
                        else {
                            break;
                        }
                    }
                    catch(e)
                    {
                        break;
                    }
                }
                
                for (var i = 1; i < table.rows.length; i++) {
                    var allData = new Array();
                    for (var j = 1; j < colName.length + 1; j++) {
                        try {
                            if (table.rows[i].cells[j].firstChild.value) {
                                count++;
                                allData.push(table.rows[i].cells[j].firstChild.value);
                            }
                            else {
                                allData.push(0);
                            }
                        }
                        catch (e) {
                            break;
                        }
                    }
                    list.push(allData);
                }
                var check = 0;
                for (var y = 0; y < list.length; y++)
                {
                    for (var z = 0; z < list[y].length; z = z + colName.length)
                    {
                        if(list[y][z] == 0)
                        {
                            swal("Year Missing");
                            check = 1;
                            break;
                        }
                    }
                }
                if(check == 0){
                    submitData(colName,list);
                }
            }
            
            function moreRecords() {
               document.getElementById("InputPlace").innerHTML = document.getElementById("tables").innerHTML;
            }
            </script>

