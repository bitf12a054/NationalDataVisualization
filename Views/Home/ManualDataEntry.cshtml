        function rowGenerater() {
            var table = document.getElementById("table");
            if (table == null) {

            }
            else {
                var rowCount = table.rows.length;
                var row = table.insertRow(rowCount);
                // serial no
                var cel1 = row.insertCell(0);
                cel1.id = "" + 0 + "" + count + "";
                console.log(cel1.id);
                cel1.innerHTML = count;

                //year
                var cel2 = row.insertCell(1);
                cel2.id = "" + 1 + "" + count + "";
                $("#" + cel2.id + "").append('<select id="' + "cel2" + cel2.id + '"></select>');
                var year = new Date();
                console.log(year.getFullYear);
                for (var i = 1947; i < year.getFullYear() + 1 ; i++) {
                    $("#cel2" + cel2.id + "").append('<option value="' + i + '">' + i + '</option>');
                }

                //Gender
                var cel3 = row.insertCell(2);
                cel3.id = "" + 2 + "" + count + "";
                $("#" + cel3.id + "").append('<select id="' + "cel3" + cel3.id + '"><option>Male</option><option>Female</option></select>');

                //Age group
                var cel4 = row.insertCell(3);
                cel4.id = "" + 3 + "" + count + "";
                $("#" + cel4.id + "").append('<select id="' + "cel4" + cel4.id + '"><option>00-04</option><option>05-09</option><option>10-14</option><option>15-19</option><option>20-24</option><option>25-29</option><option>30-34</option><option>35-39</option><option>40-44</option><option>45-49</option><option>50-54</option><option>55-59</option><option>60-64</option><option>65-69</option><option>70-74</option><option>75 And Above</option></select>');

                // noOfColumn is global variable
                for (var i = 4; i < noOfColumn ; i++) {

                    var cel = row.insertCell(i);
                    var input = document.createElement("input");
                    input.type = "text";
                    input.id = "" + i + "" + count + "";
                    input.class = "number-only ";
                    cel.appendChild(input);
                }
                $('#submit').css("visibility", "visible");
                $('#table').css("width", "90%");
            }
            count++;
        }

